{
  "name": "@poppinss/request",
  "version": "1.0.9",
  "description": "Node.js http request wrapper used by AdonisJs",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "version": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && typedoc --theme markdown --excludePrivate --mdHideSources && git add docs",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/poppinss/request.git"
  },
  "keywords": [
    "req",
    "http",
    "request",
    "adonisjs"
  ],
  "author": "virk,poppinss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poppinss/request/issues"
  },
  "homepage": "https://github.com/poppinss/request#readme",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.0.3",
    "@types/node": "^12.0.3",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "husky": "^2.3.0",
    "japa": "^2.0.10",
    "mrm": "^1.2.2",
    "np": "^5.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-markdown": "^1.2.1",
    "typescript": "^3.5.1"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/cookie": "^1.0.1",
    "accepts": "^1.3.7",
    "cuid": "^2.1.6",
    "fresh": "^0.5.2",
    "get-value": "^3.0.1",
    "macroable": "^2.0.2",
    "proxy-addr": "^2.0.5",
    "qs": "^6.7.0",
    "supertest": "^4.0.2",
    "ts-essentials": "^3.0.0",
    "type-is": "^1.6.18"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
